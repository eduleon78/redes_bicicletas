extends ../layout

block content 
  a(href='/bicicletas') volver

  br
  br
  br

  h1 Nueva Bicicleta 

  br 
  br

  form(action='/bicicletas/create' method='post')
    .form-group 
      label(for='id') ID: 
      input#id.form-control(type='text' name='id')
    .form-group
      label(for='color') Color: 
      input#color.form-control(type='text' name='color')
    .form-group
      label(for='modelo') Modelo: 
      input#modelo.form-control(type='text' name='modelo')
    .form-group
      label(for='lat') Latitud: 
      input#modelo.form-control(type='text' name='lat')
    .form-group
      label(for='lng') Longitud: 
      input#modelo.form-control(type='text' name='lng')
    br
    br
    br 
    input.btn.btn-primary(type='submit' value='OK')


